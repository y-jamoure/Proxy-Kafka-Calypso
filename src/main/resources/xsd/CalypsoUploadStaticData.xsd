	<xs:element name="LockColumns">
		<xs:annotation>
			<xs:documentation>
			Indicates if the dates are to be locked or used only for look up
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="LegType" type="xs:string" minOccurs="0"/>
				<xs:element name="LockStartDate" type="xs:boolean" minOccurs="0"/>
				<xs:element name="LockEndDate" type="xs:boolean" minOccurs="0"/>
				<xs:element name="LockPayDate" type="xs:boolean" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CustomCashflow">
		<xs:annotation>
			<xs:documentation>
			This is the definition of a Custom Cashflows for products with Simple Cashflows or Option Interest Flows.
			- An External Reference to identify a trade is necessary
			- Action specifies the action to be applied when saving the trade
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="ExternalReference"/>
						<xs:element ref="Action"/>
						<xs:element ref="CashflowSimple" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="CashflowOption" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="LockColumns" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	
	<xs:complexType name="EntityCriteria">
		<xs:sequence>
			<xs:element ref="TradeId" minOccurs="0"/>
