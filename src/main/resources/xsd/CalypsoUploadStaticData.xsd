<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0" jaxb:extensionBindingPrefixes="xjc" jaxb:version="1.0">
	<xs:include schemaLocation="CalypsoUploadTypes.xsd"/>
	<xs:include schemaLocation="CalypsoUploadTradeData.xsd"/>
	<xs:include schemaLocation="CalypsoTradeIdTypes.xsd"/>
	<xs:element name="CalypsoValuations">
		<xs:annotation>
			<xs:documentation>CalypsoValuations - contains list of 'CalypsoValuation' objects</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="CalypsoValuation" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CalypsoUserGroup">
		<xs:annotation>
			<xs:documentation>Represent the UserGroup in Calypso</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="Action"/>
						<xs:element name="Name" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Name of the Group
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ReferenceUser" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Reference user for Book.								
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Comment" type="xs:string">
							<xs:annotation>
								<xs:documentation>	
									Comment on the Group (If any).
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="FilterSet" type="xs:string">
							<xs:annotation>
								<xs:documentation>	
									Any Filter Set to apply in the Group.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ReadOnlyFilter" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Any Read only filter to be applied.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsAdmin" type="xs:boolean" default="false">
							<xs:annotation>
								<xs:documentation>
									Is the group an Admin group ? Specify this.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsSystem" type="xs:boolean" default="false">
							<xs:annotation>
								<xs:documentation>
									Is the group a System group ? Specify this.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CalypsoBOMessage">
		<xs:annotation>
			<xs:documentation>Represent the User in Calypso</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element name="TradeId" type="xs:long" minOccurs="0"/>
						<xs:element name="MessageId" type="xs:long" minOccurs="0"/>
						<xs:element name="EnteredUser" type="xs:string" minOccurs="0"/>
						<xs:element name="LegalEntity" type="xs:string" minOccurs="0"/>
						<xs:element name="Action" type="xs:string" minOccurs="0"/>
						<xs:element name="SubAction" type="xs:string" minOccurs="0"/>
						<xs:element name="MessageDate" type="DateTimeType" minOccurs="0"/>
						<xs:element name="SettleDate" type="DateTimeType" minOccurs="0"/>
						<xs:element name="MessageType" type="xs:string" minOccurs="0"/>
						<xs:element name="MessageClass" type="xs:int" minOccurs="0"/>
						<xs:element name="EventType" type="xs:string" minOccurs="0"/>
						<xs:element name="FormatType" type="xs:string" minOccurs="0"/>
						<xs:element name="ProductType" type="xs:string" minOccurs="0"/>
						<xs:element name="ProductFamily" type="xs:string" minOccurs="0"/>
						<xs:element ref="MessageAttributes"  minOccurs="0"/>
						<xs:element name="MessageDescription" type="xs:string" minOccurs="0"/>
						<xs:element name="MessageStatus" type="xs:string" minOccurs="0"/>
						<xs:element name="ReceiverInfo" type="SenderReceiverInfo" minOccurs="0"/>
						<xs:element name="SenderInfo" type="SenderReceiverInfo" minOccurs="0"/>
						<xs:element name="AddressMethod" type="xs:string" minOccurs="0"/>
						<xs:element name="Book" type="xs:string" minOccurs="0"/>
						<xs:element name="Gateway" type="xs:string" minOccurs="0"/>
						<xs:element name="TemplateName" type="xs:string" minOccurs="0"/>
						<xs:element name="Language" type="xs:string" minOccurs="0"/>
						<xs:element name="MatchingB" type="xs:boolean" minOccurs="0"/>
						<xs:element name="ExternalB" type="xs:boolean" minOccurs="0"/>
						<xs:element name="AdviceConfigId" type="xs:int" minOccurs="0"/>
						<xs:element name="TradeVersion" type="xs:int" minOccurs="0"/>
						<xs:element name="XferVersion" type="xs:int" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="CalypsoSFTRConfig">
		<xs:annotation>
			<xs:documentation>SFTR Configuration</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
							<xs:element name="Action" type="xs:string" minOccurs="0"/>
							<xs:element name="ProductType" type="xs:string" minOccurs="0"/>
							<xs:element name="ProductSubType" type="xs:string" minOccurs="0"/>
							<xs:element name="SFTRReportField" type="xs:string" minOccurs="0"/>
							<xs:element name="IsPrincipalB" type="xs:boolean" minOccurs="0"/>
							<xs:element name="CalypsoReportField" type="xs:string" minOccurs="0"/>
							<xs:element name="FieldStatus" type="xs:string" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	
	<xs:complexType name="SenderReceiverInfo">
			<xs:sequence>
				<xs:element name="Role" type="xs:string"/>
				<xs:element name="ContactType" type="xs:string"/>
				<xs:element name="AddressCode" type="xs:string"/>
			</xs:sequence>
	</xs:complexType>
	<xs:element name="MessageAttributes">
		<xs:annotation>
			<xs:documentation>Saving multiple cash account details</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MessageAttribute" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
		<xs:element name="MessageAttribute">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="AttributeName" type="xs:string"/>
				<xs:element name="AttributeValue" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CalypsoUserDefault">
		<xs:annotation>
			<xs:documentation>Represent the UserDefault in Calypso</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="Action"/>
						<xs:element name="UserName" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Name of the User to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="UserType" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Type of the User to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Email" type="xs:string"/>
						<xs:element name="DefaultDirectory" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Default directory to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Currency" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Currency to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Language" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									language to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TradeFilter" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Trade Filter to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TimeZone" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									time Zone to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Book" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Book to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Holidays" type="HolidayCodeType"/>
						<xs:element name="TraderName" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Trader Name to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="BookHierarchy" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Book Hierarchy to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="MarketPlace" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Market Place to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SalesPerson" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Sales person  to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SecurityCode" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Security Code to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="MarketType" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Market type to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DefaultIndex" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									DefaultIndex  to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="BondType" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Bond Type  to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="RateDecimals" type="xs:int">
							<xs:annotation>
								<xs:documentation>
									Rate Decimal  to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PricingEnv" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Pricing Environment  to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Country" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Country  to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TradeReference" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Trade Reference  to be set in User Default(E.g: ID, Ext Ref, Int Ref ).
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Locale" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Locale to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="EventFilter" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Event filter  to be set in User Default.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TradeWindowConfig" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Trade Window  to be set in User Default ( E.g: TradingPadWins, DealStation, NONE).
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="OpenCWSOnStartup" type="xs:boolean" default="false">
							<xs:annotation>
								<xs:documentation>
									Specify whether to open the CWS window on startup in user defaults(true/false).
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ClearTradeAfterSave" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>
									Specify whether to clear Trade window after saving a trade in user defaults(true/false).
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Templates" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="Template" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element ref="Attributes"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Template">
		<xs:annotation>
			<xs:documentation>Templates used for user Defaults</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ProductType"/>
				<xs:element ref="TemplateName"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ProductType">
		<xs:simpleType>
			<xs:restriction base="xs:string"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="TemplateName">
		<xs:simpleType>
			<xs:restriction base="xs:string"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CalypsoUser">
		<xs:annotation>
			<xs:documentation>Represent the User in Calypso</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="Action"/>
						<xs:element name="Name" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Name of the User to be created.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Password" type="xs:string">
							<xs:annotation>
								<xs:documentation>.
									Password for the user to be created.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="FullName" type="xs:string">
							<xs:annotation>
								<xs:documentation>	
									Full name of the user to be created.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Description" type="xs:string">
							<xs:annotation>
								<xs:documentation>	
									Description of the user.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PasswordMin" type="xs:int">
							<xs:annotation>
								<xs:documentation>
									Minumum length for the password field.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="MaxLoginAttempts" type="xs:int">
							<xs:annotation>
								<xs:documentation>
									Maximum number of login attemts allowed.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LoginIdleDays" type="xs:int">
							<xs:annotation>
								<xs:documentation>
									Allow maximum number of idle days.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ProcessingOrg" type="xs:string">
							<xs:annotation>
								<xs:documentation>	
									Processing Organization associated with the user(if any).
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Locked" type="xs:boolean" default="false">
							<xs:annotation>
								<xs:documentation>
									Specify whether to lock the user or not(true/false).
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ChangePwdNextLogin" type="xs:boolean" default="false">
							<xs:annotation>
								<xs:documentation>
									Specify whether to change password in next login(true/false).
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PwdCheckDigit" type="xs:boolean" default="false">
							<xs:annotation>
								<xs:documentation>
									Specify whether the password need digits or not(true/false).
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PwdSpecialChar" type="xs:boolean" default="false">
							<xs:annotation>
								<xs:documentation>
									Specify whether the password need special char or not(true/false).
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Groups" type="Groups"/>
						<xs:element name="GroupsToAdd" type="Groups"/>
						<xs:element name="GroupsToRemove" type="Groups"/>
						
						<xs:element ref="PasswordChange"/>
						<xs:element ref="Attributes"/>
						<xs:element name="ProcessingOrgs" type="ProcessingOrgs"/>
						<xs:element name="ProcessingOrgsToAdd" type="ProcessingOrgs"/>
						<xs:element name="ProcessingOrgsToRemove" type="ProcessingOrgs"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="PasswordChange">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Value" type="xs:int"/>
				<xs:element name="Days" type="xs:boolean"/>
				<xs:element name="Logins" type="xs:boolean"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CalypsoEligibilityRule">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="Action"/>
						<xs:element name="Name" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Name of the Eligibility Rule
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CollateralGiver" type="CollateralValues"/>
						<xs:element name="CollateralTaker" type="CollateralValues"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="CollateralValues">
		<xs:sequence>
			<xs:element name="Value" type="xs:string" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CalypsoBook">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="Action"/>
						<xs:element name="Name" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Name of the Book
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Activity" type="xs:string">
							<xs:annotation>
								<xs:documentation>Activity of the Book. 
									Possible values
									Fixed Income Trading Book
									All Business Lines
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AccountingBookName" type="xs:string">
							<xs:annotation>
								<xs:documentation>	
									Name of the accounting book for the Book.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LegalEntityName" type="xs:string">
							<xs:annotation>
								<xs:documentation>	
									Name of the Legal Entity (PO) for the Book.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="EODHour" type="xs:int">
							<xs:annotation>
								<xs:documentation>
									End of the Business day indicating the hours; required for the book.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="EODMin" type="xs:int">
							<xs:annotation>
								<xs:documentation>
									End of the Business day indicating the minutes;
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element ref="VariableEOD" minOccurs="0"/>
						<xs:element name="Currency" type="xs:string"/>
						<!--type="CurrencylType"-->
						<xs:element name="Location" type="xs:string"/>
						<xs:element ref="HolidayCode"/>
						<xs:element ref="Attributes"/>
						<xs:element name="Comment" type="xs:string" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CalypsoLegalEntityRelation">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="Action"/>
						<xs:element name="ProcessingOrg" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>
									Name of the ProcessingOrg
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="MultiLegalEntityB" type="xs:boolean" minOccurs="0">
							<xs:annotation>
								<xs:documentation>
									Multi Legal Entity Checkbox
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LegalEntity" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>	
									Name of the Legal Entity.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LegalEntityRole" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>	
									Role of the Legal Entity.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="OtherLegalEntity" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>
									Name of the Other Legal Entity.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="OtherLegalEntityRole" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>
									Role of the Other Legal Entity.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:extension>
