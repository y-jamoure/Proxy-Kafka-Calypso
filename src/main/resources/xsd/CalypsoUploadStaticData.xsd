	<xs:element name="SecFinanceTradeDefaults">
		<xs:annotation>
			<xs:documentation>SecFinanceTradeDefaults for LegalAgreement</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CounterpartyDefaults" type="xs:string" minOccurs="0"/>
				<xs:element name="CounterpartyAttributes" type="AttributesType" minOccurs="0"/>
				<xs:element name="ProductDefaults" type="xs:string" minOccurs="0"/>
				<xs:element name="ProductAttributes" type="AttributesType" minOccurs="0"/>
				<xs:element name="SecurityDefaults" type="xs:string" minOccurs="0"/>
				<xs:element name="SecurityAttributes" type="AttributesType" minOccurs="0"/>
				<xs:element name="TradeCurrencyDefaults" type="xs:string" minOccurs="0"/>
				<xs:element name="TradeCurrencyAttributes" type="AttributesType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CalypsoCurveOffsets">
		<xs:annotation>
			<xs:documentation>CalypsoCurveOffsets - contains list of 'CalypsoCurveOffset' objects</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="CalypsoCurveOffset" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CalypsoCurveOffset">
		<xs:annotation>
			<xs:documentation>This defines the curve offset to be uploaded into the curve</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="Action"/>
						<xs:element name="CurveName" type="xs:string"/>
						<xs:element name="CurveCurrency" type="xs:string"/>
						<!--type="CurrencylType"-->
						<xs:element name="CurveType" type="xs:string"/>
						<xs:element name="CurveDate" type="DateTimeType"/>
						<xs:element name="CurveSecondaryCurrency" type="xs:string" minOccurs="0"/>
						<xs:element ref="Offset" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Offset">
		<xs:annotation>
			<xs:documentation>This defines the offset type and DF</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="OffsetValue" type="xs:string"/>
				<xs:element name="DiscountFactorBid" type="xs:double" minOccurs="0"/>
				<xs:element name="DiscountFactor" type="xs:double" nillable="true"/>
				<xs:element name="DiscountFactorAsk" type="xs:double" minOccurs="0"/>
				<xs:element name="ZeroRateBid" type="xs:double" minOccurs="0"/>
				<xs:element name="ZeroRate" type="xs:double" nillable="true"/>
				<xs:element name="ZeroRateAsk" type="xs:double" minOccurs="0"/>
				<xs:element name="SpreadBid" type="xs:double" minOccurs="0"/>
				<xs:element name="Spread" type="xs:double" minOccurs="0"/>
				<xs:element name="SpreadAsk" type="xs:double" minOccurs="0"/>
				<xs:element name="Bid" type="xs:double" minOccurs="0"/>
				<xs:element name="Mid" type="xs:double" minOccurs="0"/>
				<xs:element name="Ask" type="xs:double" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="type" default="Date">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Date"/>
						<xs:enumeration value="Tenor"/>
						<xs:enumeration value="Offset"/>
						<!-- Added Offset support when this is given, 
						its similar to Date but the date is calculated adding the number of days given as offser to the Curve date -->
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CalypsoLegalEntity">
		<xs:annotation>
			<xs:documentation>Represent the Legal Entity in Calypso with different roles</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="Action"/>
						<xs:element name="ShortName" type="xs:string"/>
						<xs:element name="LongName" type="xs:string"/>
						<xs:element name="Parent" type="xs:string"/>
						<xs:element name="Country" type="xs:string"/>
						<xs:element name="InactiveDate" type="DateType"/>
						<xs:element name="User" type="xs:string"/>
						<xs:element ref="ExternalReference"/>
						<xs:element ref="HolidayCode"/>
						<xs:element name="Financial" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									Tag indicates whether the Legal Entity is Financial or Non Financial
									true - Financial
									false - Non Financial
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element ref="Status"/>
						<xs:element ref="Roles"/>
						<xs:element name="TripartySubstitutionB" type="xs:boolean" default="false"/>
						<xs:element name="Comment" type="xs:string"/>
						<xs:element ref="Attributes" minOccurs="0"/>
						<xs:element ref="Contacts" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Contacts">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Contact" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CorporateAction">
		<xs:annotation>
			<xs:documentation>Corporate Action Fields
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="Action"/>
						<xs:element name="CAProductCode" type="CAProductCode"/>
						<xs:element ref="CAEvent"/>
						<xs:element ref="CAAttributes"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CAEvent">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="EventDefinition"/>
				<xs:element ref="CADates"/>
				<xs:element ref="ModelInfo"/>
				<xs:element name="Comment" type="xs:string" minOccurs="0"/>
				<xs:element name="SpecialDividendB" type="xs:boolean" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EventDefinition">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SequenceNumber" type="xs:int" minOccurs="0"/>
				<xs:element name="SwiftCode" type="xs:string"/>
				<xs:element name="EventChoice" type="xs:string"/>
				<xs:element name="ParentCA" type="CAProductCode"/>
				<xs:element name="UnderlyingProduct" type="CAProductCode"/>
				<!-- <xs:element ref="SecProductCodes"/> -->
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CADates">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="AnnouncementDate" type="DateType"/>
				<xs:element name="ExDate" type="DateType"/>
				<xs:element name="ExDateInclusiveB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="ByTradeDateB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="RecordDate" type="DateType"/>
				<xs:element name="RecordDateInclusiveB" type="xs:boolean" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ModelInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="OptionStatus" type="xs:string"/>
				<xs:element name="OptionReference" type="xs:string"/>
				<xs:element name="SwiftEventOption" type="xs:string"/>
				<xs:element name="Model" type="xs:string"/>
				<xs:element name="Subtype" type="xs:string"/>
				<xs:element name="ByOpenTradeB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="PaymentDate" type="DateType"/>										<!--ValueDate in Calypso Product -->
				<xs:element name="AvailableDate" type="DateType"/>
				<xs:element name="SuccessPercent" type="xs:double" default="0" minOccurs="0"/>
				<xs:element name="AdjustmentCost" type="xs:double" default="0" minOccurs="0"/>			<!--AlternateAmount in Calypso Product -->
				<xs:element name="OtherAmount" type="xs:double" default="0" minOccurs="0"/>
				<xs:element name="OtherAmountPayDate" type="DateType"/>
				<xs:element name="Amount" type="xs:double" default="0" minOccurs="0"/>
				<xs:element name="Currency" type="xs:string"/>
				<xs:element name="RoundingMethod" type="xs:string"/>
				<xs:element name="NewRatio" type="xs:double" default="0" minOccurs="0"/>
				<xs:element name="OldRatio" type="xs:double" default="0" minOccurs="0"/>
				<xs:element name="PaymentCurrency" type="xs:string"/>
				<xs:element name="ActiveDate" type="DateType"/>
				<xs:element name="InactiveDate" type="DateType"/>
				<xs:element name="CashPayDate" type="DateType"/>
				<xs:element name="UpdateRelatedIssuanceB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="SecurityProduct" type="CAProductCode"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CAAttributes">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CAAttribute" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CAAttribute">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CAAttributeName" type="xs:string" minOccurs="0"/>
				<xs:element name="CAAttributeValue" type="xs:string" minOccurs="0"/>
				<xs:element name="ParentCAAttributeName" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SecProductCodes">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SecProductCode" type="CAProductCode" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CalypsoProductCodes">
		<xs:annotation>
			<xs:documentation>Used to save CalypsoProductCodes, will have multiple ProductCode tags. [Name, Value pair ]</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="Action"/>
						<xs:element ref="ProductCode" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="Name" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string"/>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="Value" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string"/>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
