			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="Action"/>
						<xs:element name="LegalAgreementId" type="xs:int" nillable="true"/>
						<xs:element name="LegalEntity" type="xs:string"/>
						<xs:element name="ProcessingOrg" type="xs:string"/>
						<xs:element name="ProductFamily" type="xs:string"/>
						<xs:element name="ProductType" type="xs:string"/>
						<xs:element ref="ProductTypes" minOccurs="0"/>
						<xs:element name="AgreementType" type="xs:string"/>
						<xs:element name="AgreementDate" type="DateType"/>
						<xs:element name="Status" type="xs:string"/>
						<xs:element name="ReferenceNumber" type="xs:string"/>
						<xs:element ref="ExternalLegalEntity"/>
						<xs:element ref="Currencies"/>
						<xs:element name="SpecialClause" type="xs:string"/>
						<xs:element ref="Attributes"/>
						<xs:element name="IsMaster" type="xs:boolean" default="false"/>
						<xs:element name="IsTrilateral" type="xs:boolean" default="false"/>
						<xs:element name="IsSecurityLendingDetailsPresent" type="xs:boolean" default="false"/>
						<xs:element ref="POChildrens"/>
						<xs:element ref="LEChildrens"/>
						<xs:element ref="AgreementDetails"/>
						<xs:element ref="AgreementSecurityLending"/>
						<xs:element ref="SecFinanceTradeDefaults" minOccurs="0"/>
