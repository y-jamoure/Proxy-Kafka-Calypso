			<xs:element name="TransferType" type="xs:string"/>
			<xs:element name="SettlementDate" type="xs:string"/>
			<xs:element name="Currency" type="xs:string"/>
			
			<xs:element name="Status" type="xs:string"/>
			<xs:element name="MessageType" type="xs:string"/>
			
		</xs:sequence>
	</xs:complexType>
	
	<xs:element name="CalypsoEntity">
		<xs:annotation>
			<xs:documentation>Represent the Legal Entity Agreement in Calypso</xs:documentation>
		</xs:annotation>
		
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
				
					<xs:sequence>
						<xs:element ref="Action"/>
						<xs:element name="EntityId" type="xs:long" minOccurs="0"/>
						<xs:element name="Entity" type="EntityCriteria"/>
						<xs:element name="EntityName" type="xs:string"/>
						<xs:element ref="TradeId" minOccurs="0"/>
						<xs:element name="Comment" type="xs:string"/>
						<xs:element ref="Attributes"/>
						<xs:element ref="Fields">
							<xs:annotation>
								<xs:documentation>This will be used if any additional filed information needs to be given e.g. For BoMessages 'MessageType' and for Transfer it can be "TransferType"</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="type" use="optional"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	
