	<xs:complexType name="CalypsoCurveUnderlyingSwapType">
		<xs:annotation>
			<xs:documentation>
		        In case of Swap underlying - elements that are referred as OneSide corresponds to Fix Side elements and elements that are referred
		        as OtherSide correspond to the Floating side elements of the Swap curve underlying.
		        
		        In case of BasisSwap underlying - elements that are referred as OneSide corresponds to Base Side elements and elements that are referred
		        as OtherSide correspond to the Basis side elements of the Basis Swap curve underlying.
		        </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CurveUnderlyingObject">
				<xs:sequence>
					<xs:element name="OneSideCouponFrequency" type="xs:string"/>
					<!--type="FrequencyType"-->
					<xs:element name="OneSideDateRoll" type="xs:string"/>
					<!--type="DateRollType"-->
					<xs:element name="OneSideCouponHoliday" type="HolidayCodeType"/>
					<xs:element name="OneSideDateLag" type="DateLagType" minOccurs="0"/>
					<xs:element name="OneSidePeriodRule" type="xs:string"/>
					<!--type="AccrualMethodType"-->
					<xs:element name="OneSideCompoundingFrequency" type="xs:string"/>
					<!--type="FrequencyType"-->
					<xs:element name="OneSideFormula" type="xs:string" />
					<xs:element name="OneSideRollingDay" type="xs:string" minOccurs="0"/>
					<xs:element name="OneSideRollDay" type="xs:int" minOccurs="0"/>
					<xs:element name="OtherSideRollingDay" type="xs:string" minOccurs="0"/>
					<xs:element name="OtherSideRollDay" type="xs:int" minOccurs="0"/>
					<xs:element name="OtherSideCouponFrequency" type="xs:string"/>
					<!--type="FrequencyType"-->
					<xs:element name="OtherSideDayCount" type="xs:string" nillable="true"/>
					<!-- type="DayCountType" -->
					<xs:element name="OtherSideDateRoll" type="xs:string"/>
					<!--type="DateRollType"-->
					<xs:element name="OtherSideCouponHoliday" type="HolidayCodeType"/>
					<xs:element name="OtherSideDateLag" type="DateLagType" minOccurs="0"/>
					<xs:element name="OtherSidePeriodRule" type="xs:string"/>
					<!--type="AccrualMethodType"-->
					<xs:element name="OtherSideCompoundingFrequency" type="xs:string"/>
					<!--type="FrequencyType"-->
					<xs:element name="OtherSideFormula" type="xs:string"/>
					<xs:element name="OtherSideResetTiming" type="xs:string" minOccurs="0"/>
					<xs:element name="PrincipalActual" type="xs:boolean" default="false"/>
					<xs:element name="ManualFirstReset" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>In case of Swap CU - this field is manual first reset, whereas in case of Basis swap CU
		                         it refers to the base side manual first reset</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpecificLagB" type="xs:boolean" default="false">
						<xs:annotation>
							<xs:documentation>In case of Basis Swap CU - refers to the base side specific lag boolean variable</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpecificLagOffset" type="xs:integer">
						<xs:annotation>
							<xs:documentation>In case of Basis Swap CU - refers to the base side specific lag offset variable</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpecificLagHoliday" type="HolidayCodeType">
						<xs:annotation>
							<xs:documentation>In case of Basis Swap CU - refers to the base side specific lag holiday</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!--<xs:element name="SpecificHolidayIsBusiness" type="xs:boolean" default="false"/>-->
					<xs:element name="CheckFirstResetB" type="xs:boolean" minOccurs="0"/>
					<xs:element name="InterestMethod" type="xs:string" nillable="true"/>
					<xs:element name="ForwardStart" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="CalypsoCurveUnderlying" type="CalypsoCurveUnderlyingType"/>
	<xs:complexType name="CalypsoCurveUnderlyingType">
