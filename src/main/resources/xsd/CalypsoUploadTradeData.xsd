	<xs:element name="EquityStructuredOption">
		<xs:annotation>
			<xs:documentation>
				Structured  Options where the underlying is Equity.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<!-- TradeConfig details-->
						<xs:element ref="TradeConfig"/>
						<!-- TradeConfig details finishes here-->
						<!--underlying details-->
						<xs:element ref="UnderlyingDetails"/>
						<!--underlying details finishes here-->
						<!--TradePayment details-->
						<xs:element ref="TradePayment"/>
						<!--TradePayment finishes here-->
						<!-- VanillaParameters starts here-->
						<xs:element ref="VanillaParameters"/>
						<!-- VanillaParameters finishes here-->
						<!-- CliquetParameters starts here-->
						<xs:element name="CliquetParameters" type="CliquetParameters" minOccurs="0"/>
						<!-- CliquetParameters finishes here-->
						<!-- BarrierDetails starts here-->
						<xs:element ref="BarrierDetails"/>
						<!-- BarrierDetails finishes here-->
						<!-- ForexDetails starts here-->
						<xs:element ref="ForexDetails"/>
						<!-- ForexDetails finishes here-->
						<xs:element name="DigitalType" type="xs:string" minOccurs="0"/>
						<xs:element name="DigitalAmount" type="xs:double" minOccurs="0"/>
						<!-- Pricing sheet EquityOption Strategy starts here-->
						<xs:element name="TriggerDuration" type="xs:string" minOccurs="0"/>
						<xs:element name="TriggerType" type="xs:string" minOccurs="0"/>
						<xs:element name="Trigger1" type="xs:double" minOccurs="0"/>
						<xs:element name="Trigger2" type="xs:double" minOccurs="0"/>
						<xs:element name="ObservationSchedule" type="ObservationType" minOccurs="0"/>
						<!-- Pricing sheet EquityOption Strategy ends here-->
						<xs:element ref="AsianLookBackDetails"/>
						<xs:element ref="ResetSchedules"/>
						<xs:element ref="ProductCode" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="MultiplicativeCliquetParameters" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="InterestRateStructuredOption">
		<xs:annotation>
