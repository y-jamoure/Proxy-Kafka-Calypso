<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0" jaxb:extensionBindingPrefixes="xjc" jaxb:version="1.0">
	<xs:include schemaLocation="CalypsoUploadTypes.xsd"/>
	<xs:include schemaLocation="CalypsoTradeIdTypes.xsd"/>
	<xs:simpleType name="ProductType">
		<xs:annotation>
			<xs:documentation>
				The list of product types supported by the uploader; for every product type listed below  
				there will be a Validator and Uploader java classes written which are picked up by the framework
				and are called by the Validate and Load Workflow Rules.
				
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CreditDefaultSwap"/>
			<xs:enumeration value="CDSIndex"/>
			<xs:enumeration value="CDSABSIndex"/>
			<xs:enumeration value="CDSNthLoss"/>
			<xs:enumeration value="CDSNthDefault"/>
			<xs:enumeration value="Bond"/>
			<xs:enumeration value="BondAssetBacked"/>
			<xs:enumeration value="SimpleTransfer"/>
			<xs:enumeration value="ClearingTransfer"/>
			<xs:enumeration value="FX"/>
			<xs:enumeration value="ETO"/>
			<xs:enumeration value="Equity"/>
			<xs:enumeration value="EquitySwap"/>
			<xs:enumeration value="InterestRateSwap"/>
			<xs:enumeration value="CapFloor"/>
			<xs:enumeration value="VarianceSwap"/>
			<xs:enumeration value="CancellableSwap"/>
			<xs:enumeration value="BasisSwap"/>
			<xs:enumeration value="CMSSwap"/>
			<xs:enumeration value="FRA"/>
			<xs:enumeration value="Swaption"/>
			<xs:enumeration value="OTCEquityOption"/>
			<xs:enumeration value="EquityStructuredOption"/>
			<xs:enumeration value="InterestRateStructuredOption"/>
			<xs:enumeration value="Future"/>
			<xs:enumeration value="FutureOption"/>
			<xs:enumeration value="CDSIndexTranche"/>
			<xs:enumeration value="CreditDefaultSwapABS"/>
			<xs:enumeration value="SingleSwapLeg"/>
			<xs:enumeration value="StructuredFlows"/>
			<xs:enumeration value="TransferAgent"/>
			<xs:enumeration value="CallNotice"/>
			<xs:enumeration value="Warrant"/>
			<xs:enumeration value="CreditDefaultSwaption"/>
			<xs:enumeration value="MarginCall"/>
			<xs:enumeration value="CollateralExposure"/>
			<xs:enumeration value="SecLending"/>
			<xs:enumeration value="Repo"/>
			<xs:enumeration value="CommodityOTCOption"/>
			<xs:enumeration value="FXOption"/>
			<xs:enumeration value="CommodityForward"/>
			<xs:enumeration value="CommoditySwap"/>
			<xs:enumeration value="StructuredProduct"/>
			<xs:enumeration value="FXSwap"/>
			<xs:enumeration value="FXOptionForward"/>
			<xs:enumeration value="EquityForward"/>
			<xs:enumeration value="AssetSwap"/>
			<xs:enumeration value="Advance"/>
			<xs:enumeration value="ExtendibleSwap"/>
			<xs:enumeration value="CDSIndexOption"/>
			<xs:enumeration value="BondExoticNote"/>
			<xs:enumeration value="PerformanceSwap"/>
			<xs:enumeration value="BankingFlows"/>
			<xs:enumeration value="InterestBearing"/>
			<xs:enumeration value="ContractForDifference"/>
			<xs:enumeration value="FXFlexiForward"/>
			<xs:enumeration value="DividendSwap"/>
			<xs:enumeration value="PortfolioSwap"/>
			<xs:enumeration value="CreditFacility"/>
			<!-- DTUP 1611 Added BondExotic Note-->
			<!-- HD 67552 Added CDSIndexTranche -->
			<!-- HD 75675 Added SingleSwapLeg -->
			<!-- HD 78086 Added CallNotice -->
			<!-- HD 7958 Added BankingFlows -->
			<!-- DTUP-8621 -->
			<xs:enumeration value="CreditTranche"/>
			<!-- DTUP-8622 -->
			<xs:enumeration value="CreditDrawDown"/>
			<xs:enumeration value="ContingentCreditDefaultSwap"/>
			<xs:enumeration value="TripartyExposure"/>
			<xs:enumeration value="FXAveragingForward"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ProductSubType">
		<xs:annotation>
			<xs:documentation>
				The list of product types with subtypes if applicable supported by the uploader; for every product type listed below
				there will be a Validator and Uploader java classes written which are picked up by the framework
				and are called by the Validate and Load Workflow Rules.
				e.g.:- 
						<xs:ProductType>InterestRateSwap</xs:ProductType>
				<xs:ProductSubType/>
				       				or
				       	<xs:ProductType>InterestRateSwap</xs:ProductType>
				<xs:ProductSubType>Standard</xs:ProductSubType>
				       	
				       	will use 
				       	ValidateCalypsoTradeInterestRateSwap for validations
				       	UploadCalypsoTradeInterestRateSwap for Uploading.
				       	
				       	<xs:ProductType>InterestRateSwap</xs:ProductType>
				<xs:ProductSubType>CrossCurrencySwap</xs:ProductSubType>
				       	
				       	will use 
				       	ValidateCalypsoTradeCrossCurrencySwap for validations
				       	UploadCalypsoTradeCrossCurrencySwap for Uploading.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CreditDefaultSwapLoan"/>
			<xs:enumeration value="RecoverySwap"/>
			<xs:enumeration value="CrossCurrencySwap"/>
			<xs:enumeration value="CancellableSwap"/>
			<xs:enumeration value="BasisSwap"/>
			<xs:enumeration value="CMSSwap"/>
			<xs:enumeration value="OISSwap"/>
			<xs:enumeration value="ZCInflationSwap"/>
			<xs:enumeration value="Standard"/>
			<xs:enumeration value="FXSpot"/>
			<xs:enumeration value="FXForward"/>
			<xs:enumeration value="FXForwardStart"/>
			<xs:enumeration value="FXForwardStartNDF"/>
			<xs:enumeration value="FXNDF"/>
			<xs:enumeration value="FutureEquity"/>
			<xs:enumeration value="FutureEquityIndex"/>
			<xs:enumeration value="NonDeliverableSwap"/>
			<xs:enumeration value="ExoticSwap"/>
			<xs:enumeration value="SecLending"/>
			<xs:enumeration value="Repo"/>
			<xs:enumeration value="CommodityOTCOption"/>
			<xs:enumeration value="Commodity"/>
			<xs:enumeration value="FXSwap"/>
			<xs:enumeration value="FXOptionForward"/>
			<xs:enumeration value="ExtendibleSwap"/>
			<xs:enumeration value="CappedSwap"/>
			<xs:enumeration value="FXNDFSwap"/>
			<xs:enumeration value="WindowForward"/>
			<!-- START:  Added the below field for adding support of Exotic leg-->
			<!-- END:  Added the below field for adding support of Exotic leg-->
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="UnderlyingType">
		<xs:annotation>
			<xs:documentation>This Field signifies the underlying for the ETO / Equity Swap</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value=""/>
				<xs:enumeration value="Equity"/>
				<xs:enumeration value="EquityIndex"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Type">
		<xs:annotation>
			<xs:documentation>Tells the Type of the Product [Cap, Floor, SimpleTransfer, VarianceSwap, Corporate Action]</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="TradeKeywords">
		<xs:annotation>
			<xs:documentation>Used to save Keywords, will have multiple keyword tags. [Name, Value pair ]</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Keyword" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TradeFee">
		<xs:annotation>
			<xs:documentation>Collection of Fees to be saved will have multiple Fee tags</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Fee" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TerminationReason">
		<xs:annotation>
			<xs:documentation>Used to set the keyword TerminationReason when doing a TERMINATION / PARTIALTERMINATION</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Assigned"/>
				<xs:enumeration value="BookTransfer"/>
				<xs:enumeration value="BoughtBack"/>
				<xs:enumeration value="ContractRevision"/>
				<xs:enumeration value="Manual"/>
				<xs:enumeration value="NotionalIncrease"/>
				<xs:enumeration value="Novation"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="TerminationAmount" nillable="true">
		<xs:annotation>
			<xs:documentation>The Amount being terminated from the trade.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:double"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="RemainingNotional" nillable="true">
		<xs:annotation>
			<xs:documentation>The New Amount on child trade after partial termination Partial Novation.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:double"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Termination">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TerminationReason"/>
				<xs:element name="TerminationDateTime" type="DateTimeType"/>
				<xs:element name="TerminationEffectiveDate" type="DateType"/>
				<xs:element ref="TerminationAmount"/>
				<xs:element name="TerminationVarianceNotional" type="xs:double" minOccurs="0"/>
				<xs:element ref="RemainingNotional"/>
				<xs:element name="RemainingVarianceNotional" type="xs:double" minOccurs="0"/>
				<xs:element name="TerminationLeg" type="PayRecType"/>
				<!-- Termination leg is required in case of a XCCY Swap to identify the leg to be terminated.
						 pay indicates Termination Leg is the pay leg
						 rec indicates Termination Leg is the rec leg -->
				<xs:element name="TerminationPercent" type="PercentageType" nillable="true"/>
				<!--  Added to hold value for Full First Calculation Period, setting default as true -->
				<xs:element name="FFCPOption" type="xs:boolean" minOccurs="0"/>
				<xs:element name="TerminationPrice" type="xs:double" default="0" minOccurs="0"/>
				<xs:element name="TerminationFXRate" type="xs:double" nillable="true" minOccurs="0"/>
				<!--  Added to hold value for FxFlexiForward product termination -->
				<xs:element name="MarketInfoSpotRate" type="xs:double" default="0" minOccurs="0"/>
				<xs:element name="MarketInfoSwapPointsAtActionMaturity" type="xs:double" minOccurs="0"/>
				<xs:element name="MarketInfoSwapPointsAtFlexiFwdMaturity" type="xs:double" minOccurs="0"/>
				<xs:element name="FeeInBaseCurrencyB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="FundingMargin" type="xs:double" minOccurs="0"/>
				<xs:element name="SalesMarginPoints" type="xs:double" minOccurs="0"/>
				<xs:element name="FeeCurrency" type="xs:string" minOccurs="0"/>
				<xs:element name="Book" type="xs:string" minOccurs="0"/>
				<xs:element name="NegotiatedCurrency" type="xs:string" minOccurs="0"/>
				<xs:element name="PayInterFlowsB" type="xs:boolean" minOccurs="0"/>
				<!--SecFinance is currently used for Repo trade only-->
				<xs:element ref="SecFinanceDetails" minOccurs="0"/>
				<!-- for xccy and swapXccy only -->
				<xs:element name="ExchangePrincipalB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="IncludeFeesB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="ExplodeFeesB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="DestinationBook" type="xs:string" minOccurs="0"/>
				<xs:element name="InterestCleanupB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="PenaltyFeeAmount" type="xs:double" minOccurs="0"/>
				<xs:element name="FlatPrice" type="xs:double" minOccurs="0"/>
				<xs:element ref="SettleInterestAmount" minOccurs="0"/>
				<xs:element name="PayTerminationFixingRate" type="xs:double" minOccurs="0"/>
				<xs:element name="ReceiveTerminationFixingRate" type="xs:double" minOccurs="0"/>
				<xs:element name="SpotRate" type="xs:double" minOccurs="0"/>
				<xs:element name="WorstPoints" type="xs:double" minOccurs="0"/>
				<xs:element name="NegotiatedAmount" type="xs:double" minOccurs="0"/>
				<xs:element name="UseDiscountingB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="BaseCurrencyFXRate" type="xs:double" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="type" type="xs:string" use="optional" default="Notional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="SettleInterestAmount" type="xs:double"/>
	<xs:element name="SecFinanceDetails">
		<xs:annotation>
			<xs:documentation>Collection of collateral details, containing the underlying product code and value, along with the pricers.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CollateralDetail" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CollateralDetail">
		<xs:annotation>
			<xs:documentation>
				This is used for specifying the collateral details(containing the underlying product code and value, along with the pricers).</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ProductCodeType" type="xs:string" minOccurs="0"/>
				<xs:element name="ProductCodeValue" type="xs:string" minOccurs="0"/>
				<xs:element name="ClosingPrice" type="xs:double" minOccurs="0"/>
				<xs:element name="InterestPrice" type="xs:double" minOccurs="0"/>
				<xs:element name="YieldPrice" type="xs:double" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TakeUpDetails">
		<xs:complexType>
			<xs:sequence>
				<!--xs:element name="LinkedTradeExternalReference" type="xs:string" minOccurs="0"/-->
				<xs:element name="TakeUpDate" type="DateType" minOccurs="0"/>
				<xs:element name="SettleDate" type="DateType" minOccurs="0"/>
				<xs:element name="Book" type="xs:string" minOccurs="0"/>
				<xs:element name="EffectiveRate" type="xs:double" minOccurs="0"/>
				<xs:element name="NegotiatedCurrency" type="xs:string" minOccurs="0"/>
				<xs:element name="NegotiatedAmount" type="xs:double" minOccurs="0"/>
				<xs:element name="AlternateSettleDate" type="DateType" minOccurs="0"/>
				<xs:element name="IncludeSightBillSpreadB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="MarketInfoSpotRate" type="xs:double" minOccurs="0"/>
				<xs:element name="MarketInfoSwapPointsAtActionMaturity" type="xs:double" minOccurs="0"/>
				<xs:element name="MarketInfoSwapPointsAtFlexiFwdMaturity" type="xs:double" minOccurs="0"/>
				<xs:element name="MarkUtilizedB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="SwapCostAdjustmentB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="FeeAdjustmentB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="FeeInBaseCurrencyB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="UseDiscountingB" type="xs:boolean" minOccurs="0"/>
				<xs:element name="FundingMargin" type="xs:double" minOccurs="0"/>
				<xs:element name="SalesMarginPoints" type="xs:double" minOccurs="0"/>
				<xs:element name="FeeCurrency" type="xs:string" minOccurs="0"/>
				<xs:element name="BaseCurrencyFXRate" type="xs:double" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RollDetails">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="InternalReference" minOccurs="0"/>
				<xs:element name="ActiveAtDatetime" type="DateTimeType"/>
				<!--RollOver fields for FXFlexiForward trades starts -->
				<xs:element name="RollToDate" type="DateType"/>
				<xs:element name="MarketInfoSpotRate" type="xs:double" minOccurs="0"/>
				<xs:element name="MarketInfoSwapPointsAtActionMaturity" type="xs:double" minOccurs="0"/>
				<xs:element name="MarketInfoSwapPointsAtFlexiFwdMaturity" type="xs:double" minOccurs="0"/>
				<xs:element name="NegotiatedAmount" type="xs:double" minOccurs="0"/>
				<!--RollOver fields for FXFlexiForward trades ends -->
				<xs:choice>
					<!--RollOver & RollBack fields for FX trades -->
					<xs:sequence>
						<xs:element name="Counterparty" type="xs:string"/>
						<xs:element name="Book" type="xs:string"/>
						<xs:element name="RollDateTime" type="DateTimeType"/>
						<xs:element name="NegotiatedCcy" type="xs:string"/>
						<xs:element name="SwapLegType" type="xs:string" minOccurs="0"/>
						<xs:element name="Amount" type="xs:double"/>
						<xs:element name="Reason" type="xs:string"/>
						<xs:element name="NovateTo" type="xs:string"/>
						<xs:element name="RolloverType" type="xs:string"/>
						<xs:element name="FundingRate" type="xs:double" default="0"/>
						<xs:element name="FundingMarging" type="xs:double" default="0"/>
						<xs:element name="TerminationDate" type="DateType"/>
						<xs:element name="CurrentSpot" type="xs:double" default="0"/>
						<xs:element name="CustomerNearRate" type="xs:double" minOccurs="0"/>
						<xs:element name="CustomerFarRate" type="xs:double" minOccurs="0"/>
						<xs:element name="NearPoints" type="xs:double" default="0"/>
						<xs:element name="FarPoints" type="xs:double" default="0"/>
						<xs:element name="ForwardRate" type="xs:double" default="0"/>
						<xs:element name="NearMarginPoints" type="xs:double" default="0"/>
						<xs:element name="FarMarginPoints" type="xs:double" default="0"/>
						<xs:element name="SalesMarginPoints" type="xs:double" default="0"/>
						<xs:element name="FXSplitRateB" type="xs:boolean" minOccurs="0"/>
						<xs:element name="FXReset" type="xs:string"/>
						<xs:element name="FXResetDate" type="DateType"/>
						<xs:element name="FXSplit1ResetDate" type="DateType" minOccurs="0"/>
						<xs:element name="FXSplit2ResetDate" type="DateType" minOccurs="0"/>
						<xs:element name="CurrencySplitRateB" type="xs:boolean" default="false"/>
						<xs:element ref="CurrencySplitRateDetails" minOccurs="0"/>
						<xs:element name="QuantoSplitRateB" type="xs:boolean" minOccurs="0"/>
						<xs:element name="QuantoFXResetDate" type="DateType" minOccurs="0"/>
						<xs:element name="QuantoFXResetName" type="xs:string" minOccurs="0"/>
						<xs:element name="QuantoSplit1FXResetDate" type="DateType" minOccurs="0"/>
						<xs:element name="QuantoSplit2FXResetDate" type="DateType" minOccurs="0"/>
						<xs:element name="FixingPoints" type="xs:double" minOccurs="0"/>
						<xs:element name="FixingPercent" type="xs:double" minOccurs="0"/>
						<xs:element name="ForwardStartResetRate" type="xs:string" minOccurs="0"/>
						<xs:element name="ForwardStartResetDate" type="DateType" minOccurs="0"/>
						<xs:element ref="FXFlexiForwardSchedules" minOccurs="0"/>
						<xs:element name="Frequency" type="xs:string" minOccurs="0"/>
						<xs:element name="Stub" type="xs:string" minOccurs="0"/>
						<xs:element name="TakeUpType" type="xs:string" minOccurs="0"/>
						<xs:element name="SpotRate" type="xs:double" minOccurs="0"/>
						<xs:element name="Points" type="xs:double" minOccurs="0"/>
						<xs:element name="TransferFeeB" type="xs:boolean" minOccurs="0"/>
						<xs:element name="TerminationFee" type="xs:double" minOccurs="0"/>
						<xs:element name="FeeTransferBook" type="xs:string" minOccurs="0"/>
					</xs:sequence>
					<xs:sequence>
						<!--RollOver & RollBack fields for Repo,SecLending & Cash trades -->
						<xs:element name="RollBackB" type="xs:boolean" minOccurs="0"/>
						<xs:element name="RepricedB" type="xs:boolean" minOccurs="0"/>
						<xs:element name="StartDate" type="DateType" minOccurs="0"/>
						<xs:element name="EndDate" type="DateType" minOccurs="0"/>
						<xs:element name="OpenTermB" type="xs:boolean" minOccurs="0"/>
						<xs:element name="FixedRate" type="xs:double" minOccurs="0"/>
						<xs:element name="Principal" type="xs:double" minOccurs="0"/>
						<xs:element name="CleanUpInterestB" type="xs:boolean" minOccurs="0"/>
						<xs:element name="CleanPrice" type="xs:double" minOccurs="0"/>
						<xs:element name="DirtyPrice" type="xs:double" minOccurs="0"/>
						<xs:element name="Spread" type="xs:double" minOccurs="0"/>
						<xs:element name="SalesMargin" type="xs:double" minOccurs="0"/>
						<xs:element name="SecurityQuantity" type="xs:double" minOccurs="0"/>
						<xs:element name="AgreementDate" type="DateType" minOccurs="0"/>
						<xs:element name="EffectiveDate" type="DateType" minOccurs="0"/>
						<xs:element ref="Fields"/>
					</xs:sequence>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="FIRollOverDetails">
		<xs:annotation>
			<xs:documentation>
				FIRollover/FIRollback fields for Repo,SecLending and Cash trades.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CalypsoObject">
				<xs:sequence>
					<!--RollOver & RollBack fields for Repo,SecLending, Cash trades & StructuredFlows -->
					<xs:element name="RollBackB" type="xs:boolean" minOccurs="0"/>
					<xs:element name="RepricedB" type="xs:boolean" minOccurs="0"/>
					<xs:element name="StartDate" type="DateType" minOccurs="0"/>
					<xs:element name="EndDate" type="DateType" minOccurs="0"/>
					<xs:element name="OpenTermB" type="xs:boolean" minOccurs="0"/>
					<xs:element name="FixedRate" type="xs:double" minOccurs="0"/>
					<xs:element name="Principal" type="xs:double" minOccurs="0"/>
					<xs:element name="CleanUpInterestB" type="xs:boolean" minOccurs="0"/>
					<xs:element name="CleanPrice" type="xs:double" minOccurs="0"/>
					<xs:element name="DirtyPrice" type="xs:double" minOccurs="0"/>
					<xs:element name="Spread" type="xs:double" minOccurs="0"/>
					<xs:element name="SalesMargin" type="xs:double" minOccurs="0"/>
					<xs:element name="SecurityQuantity" type="xs:double" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DividendSwap">
		<xs:annotation>
			<xs:documentation>Dividend Swap Definition
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element name="NotionalBasedB" type="xs:boolean"/>
						<xs:element name="Quantity" type="xs:double" minOccurs="0"/>
						<xs:element name="RetroRate" type="xs:double" minOccurs="0"/>
						<xs:element name="SpecialDividendB" type="xs:boolean"/>
						<xs:element ref="UnderlyingProductDetails"/>
						<xs:element name="Strike" type="xs:double" minOccurs="0"/>
						<xs:element name="Frequency" type="xs:string" minOccurs="0"/>
						<xs:element name="DateLag" type="DateLagType" minOccurs="0"/>
						<xs:element name="PaymentCurrency" type="xs:string" minOccurs="0"/>
