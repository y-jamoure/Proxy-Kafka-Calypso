	<xs:element name="CreditDefaultSwap">
		<xs:annotation>
			<xs:documentation>
				This is used for both CDS and CDSLoan; SecuredList and Cancellable is required only for CDSLoan.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CalypsoObject">
					<xs:sequence>
						<xs:element ref="Spread"/>
						<xs:element name="Issuer" type="xs:string"/>
						<xs:element ref="Seniority"/>
						<xs:element name="StubPeriod" type="xs:string" nillable="true"/>
						<!--type="StubPeriodType"-->
						<xs:element name="FirstPaymentDate" type="DateType"/>
						<xs:element ref="SpecificLastDate" minOccurs="0"/>
						<xs:element name="PaymentDayCount" type="xs:string"/>
						<!--type="DayCountType"/> -->
						<xs:element name="AccrualMethod" type="xs:string"/>
						<!--type="AccrualMethodType"-->
						<xs:element name="DateRoll" type="xs:string"/>
						<!--type="DateRollType"-->
						<xs:element name="Frequency" type="xs:string"/>
						<!--type="FrequencyType"-->
						<xs:element ref="Funded"/>
						<xs:element ref="MarketStandard"/>
						<xs:element name="SettlementMatrix" type="xs:string" minOccurs="0"/>
						<xs:element name="ISDADefinitionYear" type="xs:string" minOccurs="0"/>
						<xs:element ref="RestructuringType"/>
						<xs:element ref="TradedSpread"/>
						<xs:element ref="TradedSpreadType"/>
						<xs:element name="UpfrontPercentage" type="PercentageType" nillable="true"/>
						<xs:element ref="RecoveryRate"/>
						<xs:element name="TerminationPayment" type="xs:string" nillable="true"/>
						<!-- type="TerminationPaymentType" -->
						<xs:element ref="ProductCodeType"/>
						<xs:element ref="ProductCodeValue"/>
						<xs:element ref="SecuredList" minOccurs="0"/>
						<xs:element ref="Cancellable" minOccurs="0"/>
						<!--Settle Lag tag is only used for Calypso version 14 and above -->
						<!--Setting this to default value 3, so that if the tag is empty and no value is provided that in upload class we get 3 instead of 0-->
						<xs:element name="SettlementLag" type="xs:int" default="3" minOccurs="0"/>
						<xs:element name="PremiumLegDetails" type="PremiumLegType" minOccurs="0"/>
						<xs:element name="CDSProtectionLeg" type="CDSProtectionLeg" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
